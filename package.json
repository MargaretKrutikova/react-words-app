{
  "name": "react-template-project",
  "version": "1.0.0",
  "description": "ReactJS Template Project",
  "main": "src/index.js",
  "scripts": {
    "nodemon": "nodemon --exec babel-node server.js --plugins=transform-class-properties,transform-object-rest-spread --presets=react,env --env={'targets': {'node': 'current' }} --ignore public/",
    "webpack": "webpack -wd",
    "build-webpack": "cross-env NODE_ENV=production webpack -p",
    "test": "cross-env NODE_ENV=test jest --watchAll",
    "dev": "webpack-dev-server --hot --history-api-fallback --open --config webpack.config.hot.js"
  },
  "author": "Margarita Krutikova <margaretkru@gmail.com>",
  "repository": "https://github.com/margaretkrutikova/react-template-project",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.4",
    "nodemon": "^1.14.3",
    "react-hot-loader": "^3.1.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-polyfill": "^6.26.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}

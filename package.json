{
  "name": "react-template-project",
  "version": "1.0.0",
  "description": "ReactJS Template Project",
  "main": "src/index.js",
  "scripts": {
    "nodemon": "nodemon --exec babel-node server.js --ignore public/",
    "webpack": "webpack -wd",
    "build-webpack": "webpack -p",
    "test": "jest --watchAll",
    "dev": "webpack-dev-server --hot --history-api-fallback --open --config webpack.config.hot.js"
  },
  "author": "Margarita Krutikova <margaretkru@gmail.com>",
  "repository": "https://github.com/margaretkrutikova/react-template-project",
  "license": "MIT",
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "react-hot-loader": "^3.1.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-polyfill": "^6.26.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  }
}

{
  "name": "react-template-project",
  "version": "1.0.0",
  "description": "ReactJS Template Project",
  "main": "src/index.js",
  "scripts": {
    "nodemon": "nodemon --exec babel-node server.js --plugins=transform-class-properties,transform-object-rest-spread --presets=react,env --env={'targets': {'node': 'current' }} --ignore public/",
    "webpack": "webpack -wd",
    "build-webpack": "./node_modules/.bin/cross-env NODE_ENV=production webpack -p",
    "test": "cross-env NODE_ENV=test jest --watchAll",
    "dev": "webpack -d --env.hot && webpack-dev-server -d --hot --history-api-fallback --open --content-base ./public"
  },
  "author": "Margarita Krutikova <margaretkru@gmail.com>",
  "repository": "https://github.com/margaretkrutikova/react-template-project",
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "^Services(.*)$": "<rootDir>/src/services$1",
      "(.*).scss$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.73.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "jest-in-case": "^1.0.2",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.5",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "^4.2.0",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "svgr": "^1.10.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "apollo-fetch": "^0.7.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.1",
    "ejs": "^2.5.7",
    "eslint-plugin-flowtype": "^2.49.3",
    "express": "^4.16.2",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-portal": "^4.1.5",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "shortid": "^2.2.8"
  }
}

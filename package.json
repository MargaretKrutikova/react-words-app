{
  "name": "react-template-project",
  "version": "1.0.0",
  "description": "ReactJS Template Project",
  "main": "src/index.js",
  "scripts": {
    "nodemon": "nodemon --exec babel-node server.js --plugins=transform-class-properties,transform-object-rest-spread --presets=react,env --env={'targets': {'node': 'current' }} --ignore public/",
    "webpack": "webpack -wd",
    "build-webpack": "cross-env NODE_ENV=production webpack -p",
    "test": "cross-env NODE_ENV=test jest --watchAll",
    "dev": "webpack -d --env.hot && webpack-dev-server -d --hot --history-api-fallback --open --content-base ./public"
  },
  "author": "Margarita Krutikova <margaretkru@gmail.com>",
  "repository": "https://github.com/margaretkrutikova/react-template-project",
  "license": "MIT",
  "jest": {
    "moduleNameMapper": {
      "^Services(.*)$": "<rootDir>/src/services$1",
      "(.*).scss$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "jest-in-case": "^1.0.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.3",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "^3.1.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "apollo-fetch": "^0.7.0",
    "axios": "^0.17.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0-beta.2",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  }
}

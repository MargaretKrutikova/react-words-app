[ignore]

[include]

[libs]
flow-typed

[lints]

[options]
module.name_mapper='^Services\([-a-zA-Z0-9$_/]+\)$' -> '<PROJECT_ROOT>/src/services/\1'
module.name_mapper='^Reducers\([-a-zA-Z0-9$_/]+\)$' -> '<PROJECT_ROOT>/src/reducers/\1'
module.name_mapper='^Common\([-a-zA-Z0-9$_/]+\)$' -> '<PROJECT_ROOT>/src/common/\1'
module.system.node.resolve_dirname=./src
module.system.node.resolve_dirname=node_modules
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'

[strict]